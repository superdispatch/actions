{
  "name": "@superdispatch/actions",
  "version": "1.0.19",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "ts-node scripts/build.ts",
    "postinstall": "husky install",
    "lint": "js-tools lint",
    "prerelease": "yarn install --frozen-lockfile && yarn tsc && yarn lint && yarn build",
    "release": "np --no-publish --no-release-draft",
    "postrelease": "yarn ts-node scripts/post-release.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "js-tools lint --fix"
  },
  "prettier": "@superdispatch/prettier-config",
  "devDependencies": {
    "@actions/cache": "^1.0.7",
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.2.0",
    "@superdispatch/eslint-plugin": "^0.3.0",
    "@superdispatch/js-tools": "^0.3.0",
    "@superdispatch/prettier-config": "^0.3.0",
    "@superdispatch/tsconfig": "^0.3.0",
    "@types/pretty-bytes": "^5.2.0",
    "esbuild": "^0.12.7",
    "eslint": "^7.28.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "np": "^7.5.0",
    "prettier": "^2.3.1",
    "pretty-bytes": "^5.6.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "zlib": "^1.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
